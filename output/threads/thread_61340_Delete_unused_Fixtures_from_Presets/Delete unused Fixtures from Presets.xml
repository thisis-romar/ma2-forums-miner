<?xml version="1.0" encoding="utf-8"?>
<MA xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.malighting.de/grandma2/xml/MA" xsi:schemaLocation="http://schemas.malighting.de/grandma2/xml/MA http://schemas.malighting.de/grandma2/xml/3.3.4/MA.xsd" major_vers="3" minor_vers="3" stream_vers="4">
	<Info datetime="2018-05-14T13:54:34" showfile="ttt" />
	<Macro index="0" name="Delete unused Fixtures from Presets">
		<Macroline index="0">
			<text>ClearAll</text>
		</Macroline>
		<Macroline index="1">
			<text>SetUserVar $Presettype=(&quot;Presettype as Number&quot;)</text>
		</Macroline>
		<Macroline index="2">
			<text>SetUserVar $Presetstart=(&quot;Preset Start-Number&quot;)</text>
		</Macroline>
		<Macroline index="3">
			<text>SetUserVar $Presetend=(&quot;Preset End-Number&quot;)</text>
		</Macroline>
		<Macroline index="4">
			<text>Search Preset $Presettype.$Presetstart If Sequence Thru /nc</text>
			<info>search for all Fixtures who use this Preset</info>
		</Macroline>
		<Macroline index="5">
			<text>SelFix SearchResult</text>
			<info>select the Fixtures who use this preset</info>
		</Macroline>
		<Macroline index="6">
			<text>Invert Fixture Thru</text>
			<info>select all other unused fixtures</info>
		</Macroline>
		<Macroline index="7">
			<text>On PresetType $Presettype</text>
			<info>activate the position presettype for them</info>
		</Macroline>
		<Macroline index="8">
			<text>Store Preset $Presettype.$Presetstart /remove</text>
			<info>store and remove them from this preset</info>
		</Macroline>
		<Macroline index="9">
			<text>ClearAll</text>
			<info>clear the programmer</info>
		</Macroline>
		<Macroline index="10">
			<text>[$Presetstart==$Presetend] Off Macro &quot;Delete unused Fixtures from Presets&quot;</text>
		</Macroline>
		<Macroline index="11">
			<text>AddUserVar $Presetstart=1</text>
		</Macroline>
		<Macroline index="12">
			<text>Go Macro 1.&quot;Delete unused Fixtures from Presets&quot;.5</text>
		</Macroline>
	</Macro>
</MA>